  "scripts": {
    "build": "webpack --display-error-details --display=minimal --progress --config webpack/index.js",
    "start": "better-npm-run start",
    "debug": "better-npm-run debug",
    "lint": "npx eslint --ext .jsx,.js,.tsx,.ts --config ./eslint.config.js --fix src/",
    "migrate": "node -r esm ./node_modules/.bin/knex migrate:latest",
    "seed": "node -r esm ./node_modules/.bin/knex seed:run",
    "pretest": "npm run lint",
    "test": "tsc"
  },
  "betterScripts": {
    "debug": {
      "comment": "This script runs the same thing as dev, but with all loggers and tooling enabled and set to the debug level. This generates far more console output. Alternatively, you can search the codebase for the __DEBUG__ webpack var and toggle loggers one by one.",
      "command": "npm run build && concurrently \"webpack-dev-server --config webpack/\" \"DEBUG=koa* node --max-old-space-size=8192 --inspect ./dist/server.js\"",
      "env": {
        "DEBUG": true,
        "NODE_ENV": "development"
      }
    },
    "start": {
      "comment": "This script bundles the initial build then starts up the dev server and localhost concurrently together. TODO get dev server fucking working jesus",
      "command": "npm run build && concurrently \"webpack --watch --config webpack/\" \"nodemon --max-old-space-size=8192 ./dist/server.js\"",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
