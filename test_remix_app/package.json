{
  "name": "test-remix-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "seed": "npx tsx ./prisma/seed/seed.ts",
    "migrate": "prisma migrate deploy",
    "postmigrate": "npx @snaplet/seed sync",
    "db:generate": "prisma generate",
    "db:pull": "prisma db pull",
    "db:seed": "prisma db seed",
    "db:seed_init": "npx @snaplet/seed init prisma/seed",
    "db:seed_sync": "npx @snaplet/seed sync",
    "db:migration_apply": "npx prisma migrate dev",
    "db:migration_deploy": "prisma migrate deploy",
    "db:migration_new": "prisma migrate dev --create-only",
    "db:migration_resolve": "prisma migrate resolve --applied",
    "db:migration_status": "prisma migrate status"
  },
  "dependencies": {
    "@prisma/client": "^5.21.1",
    "@remix-run/node": "^2.13.1",
    "@remix-run/react": "^2.13.1",
    "@remix-run/serve": "^2.13.1",
    "@snaplet/seed": "^0.98.0",
    "@tanstack/react-query": "^5.59.15",
    "classnames": "^2.5.1",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsx": "^4.19.1",
    "zod": "^3.23.8",
    "zod-prisma-types": "^3.1.8"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.13.1",
    "@snaplet/copycat": "^5.1.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "prisma": "^5.21.1",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed/seed.ts"
  },
  "@snaplet/seed": {
    "config": "prisma/seed/seed.config.ts"
  }
}